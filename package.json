{
  "dependencies": {
    "@hgraph.io/sdk": "^0.8.4",
    "dotenv": "^16.0.3"
  },
  "devDependencies": {
    "@types/jest": "^29.5.5",
    "@types/node": "^18.15.11",
    "@typescript-eslint/eslint-plugin": "^8.37.0",
    "@typescript-eslint/parser": "^8.37.0",
    "esbuild": "^0.17.15",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.5.1",
    "jest": "^29.6.1",
    "prettier": "^3.6.2",
    "ts-jest": "^29.1.1"
  },
  "scripts": {
    "watch": "sh -c \"mkdir -p dist && serve dist & nodemon -w src -e ts,js,tsx,jsx,json,gql,graphql,css,html --exec 'npm run build'& wait\"",
    "build": "node ./build.mjs",
    "start": "npm run build && node dist/index.js",
    "test": "jest --coverage",
    "lint": "eslint . --ext .ts"
