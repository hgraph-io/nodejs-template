{
  "dependencies": {
    "@hgraph.io/sdk": "^0.8.4",
    "dotenv": "^16.0.3"
  },
  "devDependencies": {
    "@types/jest": "^29.5.5",
    "@types/node": "^18.15.11",
    "esbuild": "^0.17.15",
    "jest": "^29.6.1",
    "ts-jest": "^29.1.1"
  },
  "scripts": {
    "watch": "sh -c \"mkdir -p dist && serve dist & nodemon -w src -e ts,js,tsx,jsx,json,gql,graphql,css,html --exec 'npm run build'& wait\"",
